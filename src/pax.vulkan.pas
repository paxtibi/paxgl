function TVulkanBase.SupportsExtension(const Ext: string): boolean;
var
  s: string;
begin
  Result := False;
  if Length(FExtensions) = 0 then
    loadExtensionNames;
  if Length(FExtensions) > 0 then
  begin
    for s in FExtensions do
    begin
      if UpperCase(ext) = UpperCase(s) then
        exit(True);
    end;
  end;
end;

