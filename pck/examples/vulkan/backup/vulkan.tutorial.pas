{ TPPCharHelper }

function TPPCharHelper.toArray(): TStringArray;
var
  i: integer;
  cursor, pp: PPChar;
begin
  pp := self;
  Result := nil;
  if pp = nil then exit;
  cursor := pp;
  i := 0;
  while cursor^ <> nil do
  begin
    SetLength(Result, i + 1);
    Result[i] := StrPas(cursor^);
    Inc(cursor);
    Inc(i);
  end;
end;

function TPPCharHelper.toArray(size: uint32): TStringArray;
var
  i: integer;
  PP: PPChar;
begin
  SetLength(Result, size);
  PP := self;
  for i := 0 to size - 1 do
  begin
    if pp^ = nil then
      Result[i] := ''
    else
      Result[i] := StrPas(pp^);
    Inc(pp);
  end;
end;


