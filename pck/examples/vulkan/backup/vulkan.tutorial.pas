    VK_SURFACE_TRANSFORM_HORIZONTAL_MIRROR_ROTATE_90_BIT_KHR: Result := 'H_MIRROR_ROTATE_90';
    VK_SURFACE_TRANSFORM_HORIZONTAL_MIRROR_ROTATE_180_BIT_KHR: Result := 'H_MIRROR_ROTATE_180';
