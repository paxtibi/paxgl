unit vulkan.tutorial;

{$mode objfpc}{$H+}
{$modeswitch advancedrecords}
{$modeswitch typehelpers}

{$A8}// alignment
{$Q+}// overflow checking (opzionale)
{$R+}// range checking (opzionale)
{$D+}// debug info


interface

uses
  {$IFDEF WINDOWS}  windows,{$endif}  Classes, SysUtils, StrUtils, Generics.Collections, pax.vulkan, pax.vulkan.helpers, logger;

type
  TVec2 = record
    x, y: single;

    // Costruttori / helper (opzionale ma molto comodo)
    class function Create(const ax, ay: single): TVec2; static; inline;
    class function Zero: TVec2; static; inline;

    // Operatori matematici base
    class operator +(const a, b: TVec2): TVec2;
    class operator -(const a, b: TVec2): TVec2;
    class operator *(const v: TVec2; s: single): TVec2;
    class operator *(const a, b: TVec2): TVec2;     // component-wise multiply
    class operator /(const v: TVec2; s: single): TVec2;
    class operator -(const v: TVec2): TVec2;       // unary minus

    // Metodi di utilità geometrica
    function Length: single; inline;
    function LengthSquared: single; inline;
    function Normalized: TVec2;
    function Dot(const other: TVec2): single; inline;
    function Perpendicular: TVec2; inline;          // ruota di 90° (clockwise)
    function ToString: string;
  end;

  { TVec3 }

  TVec3 = record
    x, y, z: single;
    class function Create(const ax, ay, az: single): TVec3; static; inline;
    class operator +(const a, b: TVec3): TVec3;
    class operator -(const a, b: TVec3): TVec3;
    class operator *(const v: TVec3; s: single): TVec3;
  end;

  TVertex = record
    pos: TVec2;
    color: TVec3;
  end;

  { TVec4 }

  TVec4 = record
    x, y, z, w: single;
  end;

  { TMat4 - colonna-major (come GLM / Vulkan / OpenGL) }

  TMat4 = record
  public
    m: array[0..3, 0..3] of single;
  public
    // Moltiplicazione tra due matrici
    class operator *(const a, b: TMat4): TMat4;

    // Moltiplicazione matrice × vettore
    class operator *(const mat: TMat4; const v: TVec4): TVec4;

    function Element(row, col: integer): single; inline;
    procedure SetElement(row, col: integer; Value: single); inline;

    function ToString: string;
  end;

  TMat4Array = array of TMat4;

  { IVulkanTutorial }

  IVulkanTutorial = interface
    ['{969CB0F8-0C9B-4B0F-B377-42F20F096716}']
    function GetCaption: string;
    function getLogger: TLogger;
    procedure run;
    function GetEnableValidationLayers: boolean;
    procedure SetEnableValidationLayers(AValue: boolean);
    property EnableValidationLayers: boolean read GetEnableValidationLayers write SetEnableValidationLayers;
    property Logger: TLogger read getLogger;
    property Caption: string read GetCaption;
  end;

  { TStringArrayTypeHelper }

  TStringArrayTypeHelper = type helper for TStringArray
    function toPPChar: PPChar;
    function Text: string;
    function formattedString(): string;
  end;

  { TPPCharHelper }

  TPPCharHelper = type helper for PPChar
    function toArray(): TStringArray;
    function toArray(size: uint32): TStringArray;
  end;

  { TVulkanTutorial }

  TVulkanTutorial = class(TInterfacedObject, IVulkanTutorial)
  protected
    FEnableValidationLayers: boolean;
    FCaption: string;
    FInstanceExtensions: TStringList;
    FDeviceExtensions: TStringList;
    FDeviceFeatures: TStringList;
    procedure SetCaption(AValue: string);
  protected
    procedure InitWindow; virtual; abstract;
    procedure InitVulkan; virtual; abstract;
    procedure MainLoop; virtual; abstract;
    procedure Cleanup; virtual; abstract;
  protected
    function getLogger: TLogger;
    function GetCaption: string;
  public
    constructor Create; virtual;
    destructor Destroy; override;
    procedure run; virtual;
    function GetEnableValidationLayers: boolean;
    procedure SetEnableValidationLayers(AValue: boolean);
    property EnableValidationLayers: boolean read GetEnableValidationLayers write SetEnableValidationLayers;
    property Caption: string read GetCaption write SetCaption;
  end;



