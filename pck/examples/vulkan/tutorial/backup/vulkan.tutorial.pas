procedure TMat4.SetElement(row, col: integer; Value: single);
begin
  m[row, col] := Value;
end;

function TMat4.ToString: string;
var
  i, j: integer;
begin
  Result := '';
  for i := 0 to 3 do
  begin
    for j := 0 to 3 do
      Result := Result + Format('%8.3f ', [m[i, j]]);
    Result := Result + LineEnding;
  end;
end;

class function TVec2.Create(const ax, ay: single): TVec2;
begin
  Result.x := ax;
  Result.y := ay;
end;

class function TVec2.Zero: TVec2;
begin
  Result.x := 0;
  Result.y := 0;
end;

class operator TVec2.+(const a, b: TVec2): TVec2;
begin
  Result.x := a.x + b.x;
  Result.y := a.y + b.y;
end;

class operator TVec2.-(const a, b: TVec2): TVec2;
begin
  Result.x := a.x - b.x;
  Result.y := a.y - b.y;
end;

class operator TVec2.*(const v: TVec2; s: single): TVec2;
begin
  Result.x := v.x * s;
  Result.y := v.y * s;
end;

class operator TVec2.*(const a, b: TVec2): TVec2;
begin
  Result.x := a.x * b.x;
  Result.y := a.y * b.y;
end;

class operator TVec2./(const v: TVec2; s: single): TVec2;
begin
  Result.x := v.x / s;
  Result.y := v.y / s;
end;

class operator TVec2.-(const v: TVec2): TVec2;
begin
  Result.x := -v.x;
  Result.y := -v.y;
end;

function TVec2.Length: single;
begin
  Result := Sqrt(x * x + y * y);
end;

function TVec2.LengthSquared: single;
begin
  Result := x * x + y * y;
end;

function TVec2.Normalized: TVec2;
var
  len: single;
begin
  len := Length;
  if len > 0 then
    Result := Self / len
  else
    Result := Self;
end;

function TVec2.Dot(const other: TVec2): single;
begin
  Result := x * other.x + y * other.y;
end;

function TVec2.Perpendicular: TVec2;
begin
  Result.x := -y;
  Result.y := x;
end;

function TVec2.ToString: string;
begin
  Result := Format('(%0.4f, %0.4f)', [x, y]);
end;


