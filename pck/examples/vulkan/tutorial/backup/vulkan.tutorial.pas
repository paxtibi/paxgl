function Mat4Identity: TMat4;
begin
  FillChar(Result, SizeOf(Result), 0);
  Result.m[0, 0] := 1;
  Result.m[1, 1] := 1;
  Result.m[2, 2] := 1;
  Result.m[3, 3] := 1;
end;

function Mat4Translate(const m: TMat4; const v: TVec3): TMat4;
begin
  Result := m;
  Result.m[3, 0] := Result.m[3, 0] + v.x;
  Result.m[3, 1] := Result.m[3, 1] + v.y;
  Result.m[3, 2] := Result.m[3, 2] + v.z;
end;

function Mat4Rotate(const m: TMat4; angleRad: single; const axis: TVec3): TMat4;
var
  c, s, t: single;
  ux, uy, uz: single;
  rot: TMat4;
begin
  c := Cos(angleRad);
  s := Sin(angleRad);
  t := 1 - c;
  ux := axis.x;
  uy := axis.y;
  uz := axis.z;

  FillChar(rot, SizeOf(rot), 0);
  rot.m[0, 0] := t * ux * ux + c;
  rot.m[0, 1] := t * ux * uy - s * uz;
  rot.m[0, 2] := t * ux * uz + s * uy;
  rot.m[1, 0] := t * uy * ux + s * uz;
  rot.m[1, 1] := t * uy * uy + c;
  rot.m[1, 2] := t * uy * uz - s * ux;
  rot.m[2, 0] := t * uz * ux - s * uy;
  rot.m[2, 1] := t * uz * uy + s * ux;
  rot.m[2, 2] := t * uz * uz + c;
  rot.m[3, 3] := 1;

  Result := rot * m;
end;

function Mat4Scale(const m: TMat4; const v: TVec3): TMat4;
begin
  Result := m;
  Result.m[0, 0] := Result.m[0, 0] * v.x;
  Result.m[1, 1] := Result.m[1, 1] * v.y;
  Result.m[2, 2] := Result.m[2, 2] * v.z;
end;

function Mat4Perspective(fovyDeg: single; aspect: single; zNear, zFar: single): TMat4;
var
  tanHalfFovy: single;
begin
  FillChar(Result, SizeOf(Result), 0);
  tanHalfFovy := Tan(fovyDeg * DegToRad / 2);
  Result.m[0, 0] := 1 / (aspect * tanHalfFovy);
  Result.m[1, 1] := 1 / tanHalfFovy;
  Result.m[2, 2] := zFar / (zFar - zNear);
  Result.m[2, 3] := -1;
  Result.m[3, 2] := -(zFar * zNear) / (zFar - zNear);
  Result.m[3, 3] := 0;
end;

function Cross(const a, b: TVec3): TVec3;
begin
  Result.x := a.y * b.z - a.z * b.y;
  Result.y := a.z * b.x - a.x * b.z;
  Result.z := a.x * b.y - a.y * b.x;
end;

function Dot(const a, b: TVec3): single;
begin
  Result := a.x * b.x + a.y * b.y + a.z * b.z;
end;

function Normalize(const v: TVec3): TVec3;
var
  len: single;
begin
  len := Sqrt(Dot(v, v));
  if len > 0 then
  begin
    Result.x := v.x / len;
    Result.y := v.y / len;
    Result.z := v.z / len;
  end
  else
    Result := v;
end;

function Mat4LookAt(const eye, center, up: TVec3): TMat4;
var
  f, s, u: TVec3;
begin
  f := Normalize(center - eye);
  s := Normalize(Cross(f, up));
  u := Cross(s, f);

  Result := Mat4Identity;
  Result.m[0, 0] := s.x;
  Result.m[0, 1] := s.y;
  Result.m[0, 2] := s.z;
  Result.m[1, 0] := u.x;
  Result.m[1, 1] := u.y;
  Result.m[1, 2] := u.z;
  Result.m[2, 0] := -f.x;
  Result.m[2, 1] := -f.y;
  Result.m[2, 2] := -f.z;

  Result.m[3, 0] := -Dot(s, eye);
  Result.m[3, 1] := -Dot(u, eye);
  Result.m[3, 2] := Dot(f, eye);
end;

{ Operatori TMat4 }

class operator TMat4.*(const a, b: TMat4): TMat4;
var
  i, j, k: integer;
begin
  for i := 0 to 3 do
    for j := 0 to 3 do
    begin
      Result.m[i, j] := 0;
      for k := 0 to 3 do
        Result.m[i, j] := Result.m[i, j] + a.m[i, k] * b.m[k, j];
    end;
end;

class operator TMat4.*(const mat: TMat4; const v: TVec4): TVec4;
begin
  Result.x := mat.m[0, 0] * v.x + mat.m[1, 0] * v.y + mat.m[2, 0] * v.z + mat.m[3, 0] * v.w;
  Result.y := mat.m[0, 1] * v.x + mat.m[1, 1] * v.y + mat.m[2, 1] * v.z + mat.m[3, 1] * v.w;
  Result.z := mat.m[0, 2] * v.x + mat.m[1, 2] * v.y + mat.m[2, 2] * v.z + mat.m[3, 2] * v.w;
  Result.w := mat.m[0, 3] * v.x + mat.m[1, 3] * v.y + mat.m[2, 3] * v.z + mat.m[3, 3] * v.w;
end;

